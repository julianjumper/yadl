// RUN: DEFAULT
x = [1, 2, 3, 4, 5]

mapper = (x) => {
    if (x % 2 == 0) {
         return [ 1, 2 ]
    } else {
         return [ 3 ]
    }
}
next = (data) => {
    if (data['index'] < len(data['list'])) {
        data['index'] = data['index'] + 1
        return data['list'][ data['index'] - 1 ]
    } else {
        return none
    }
}
has_next = (data) => data['index'] < len(data['list'])
d = {
   'index': 0,
   'list': x
}

print3(x)
// CHECK-OUT: [1, 2, 3, 4, 5]

iter = flatmap(iterator(next, has_next, d), mapper)

while (has_next(iter)) {
    print(next(iter))
}

// CHECK-OUT: 3
// CHECK-OUT: 1
// CHECK-OUT: 2
// CHECK-OUT: 3
// CHECK-OUT: 1
// CHECK-OUT: 2
